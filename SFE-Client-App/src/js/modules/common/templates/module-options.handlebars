        <li class="float_pop_in"><a href="#" class="unfloat-view">Pop-In</a></li>
        {{#if showEditButton}}
        <li><a href="#" class="edit-rules">Edit</a></li>
        {{/if}}
        <li><a href="#" class="show-settings">Settings</a></li>
    </ul>
